{
	"name":"SilentTweet",
	"version":"1.1",
	"manifest_version":2,
	"content_scripts": [
		{
			"matches": ["https://twitter.com/*"],
			"js": ["content.js"]
		}
	],
	"browser_action":{
		"default_popup":"popup.html",
		"default_title":"SilentTweet"
	},
	"permissions": [
		"storage"
	]
}